@model RegistrationForm_mvc.Data.SignUp
@{
    ViewBag.Title = "Profile";
}

@using (Html.BeginForm())
{
    <h2>USER PROFILE</h2>
    <br />
    <div class="container">
        <div class="form-group">
            <table>
                <tr>
                    <td><b>UserName:</b></td>
                    <td>
                        @Html.DisplayFor(x => x.FirstName)
                        &nbsp;
                        @Html.DisplayFor(x => x.LastName)
                    </td>
                    <td>
                    </td>
                </tr>
                <tr>
                    <td><b>Email Id :</b></td>
                    <td>
                        @Html.DisplayFor(x => x.Email)
                    </td>
                </tr>
                <tr>
                    <td><b> Mobile Number : </b></td>
                    <td>
                        @Html.DisplayFor(x => x.MobileNo)
                    </td>
                </tr>
                <tr>
                    <td><b>Address : </b></td>
                    <td>
                        @Html.DisplayFor(x => x.Address)
                    </td>
                </tr>
                <tr>
                    <td><b>Company Name :</b></td>
                    <td>
                        @Html.DisplayFor(x => x.ComapnyName)
                    </td>
                </tr>
                <br />
                <tr>
                    <td>
                        @Html.ActionLink("ChangePassword", "ChangePassword", "Employee")
                        <br />
                        @Html.ActionLink("SignOut", "SignOut", "Employee")
                    </td>
                </tr>

            </table>



        </div>

    </div>
}

